<%= form_for @stadium do |f| %>

  <div class="form-group">
    <%= f.label :city %><br/>
    <%= f.text_field :city %>
  </div>

  <div class="form-group">
    <%= f.label :university %><br/>
    <%= f.text_field :university %>
  </div>

  <div class="form-group">
    <%= f.label :cep %><br/>
    <%= f.text_field :cep %>
  </div>

  <div class="form-group">
    <%= f.label :street %><br/>
    <%= f.text_field :street %>
  </div>

  <div class="form-group">
    <%= f.label :number %><br/>
    <%= f.text_field :number %>
  </div>

  <div class="form-group">
    <%= f.label :state %><br/>
    <%= f.text_field :state %>
  </div>

  <div class="actions"><%= f.submit %></div>
<% end %>

