<%= form_for @stadium do |f| %>

  <div class="form-group">
    <%= f.label :city %><br/>
    <%= f.text_field :city, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :university %><br/>
    <%= f.text_field :university, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :cep %><br/>
    <%= f.text_field :cep, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :street %><br/>
    <%= f.text_field :street, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :number %><br/>
    <%= f.text_field :number, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :state %><br/>
    <%= f.text_field :state, class: "form-control" %>
  </div>

  <div class="actions"><%= f.submit "Registrar" %></div>
<% end %>

